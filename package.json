{
  "name": "totp-basic",
  "version": "0.0.9",
  "description": "A Basic TOTP (Time-based One-time Password Algorithm) implementation in TypeScript for use with cloudflare-workers",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230307.0",
    "typescript": "^5.0.2",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "run:tsnode": "node --no-warnings --loader ts-node/esm.mjs index.ts",
    "prepublishOnly": "mv dist/src/* dist/ && rm -rf dist/src && rm -rf dist/test && mv dist/* .",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
